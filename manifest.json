{
	"name": "Bookmark+",
	"version": "1.0",
	"manifest_version": 2,
	"description": "This let you to manage your bookmarks conveniently and easily.",
	"icons": {
		"16": "assets/img/icon16.png",
		"48": "assets/img/icon48.png",
		"128": "assets/img/icon128.png"
	},
	"browser_action": {
		"default_icon" : "assets/img/icon.png",
		"default_title": "Bookmark+",
		"default_popup": "popup.html"
	},
	"incognito": "spanning",
	"permissions": [
		"bookmarks",
		"tabs",
		"clipboardWrite"
	]
}